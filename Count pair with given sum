T=int(input('No. of testcases:'))
for k in range(T):
    N,K=input().split()
    N=int(N)
    K=int(K)
    A=[int(l) for l in input().split()]
    count=0
    for i in range(0,N):
        for j in range(i+1,N):
            if (A[i] + A[j] == K):
                count = count + 1
            else:
                continue
    print(count)





